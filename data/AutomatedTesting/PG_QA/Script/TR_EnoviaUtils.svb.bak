Function TR_EnoviaUtils_RoleOK()
  Set objOk = objGetObject(OR_EnoviaLogin_OKBtn,"",30000)
  objOk.Click()
  'Set objtext = objGetObject(OR_EnoviaHomepage_Responsibility,"","")
