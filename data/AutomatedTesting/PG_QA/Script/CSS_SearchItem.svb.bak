Function SelectMaterialGroup(strGroup, strType)
    Dim strMgroup
    'Dim arrGetObj,classObj, objMaterial,objMaterialList,objSave
    'Get Material Group Attribute value for input from excel
    Dim MG 
    Set MG = MaterialGroup(strType)
