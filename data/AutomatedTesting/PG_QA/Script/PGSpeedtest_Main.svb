'USEUNIT PGSpeedTest
'USEUNIT FileLib


'PG Speedtest NA
Function Start_PGSpeedtest_NA_Manual()
  Call Start_PGSpeedtest_NA()
End Function
Function Start_PGSpeedtest_NA()
  While 1  
    nMinutes = aqDateTime.GetMinutes(aqDateTime.Now)
    x = nMinutes MOD 2
    If x = 0 then
      Call PGSpeedtest_NA()
    Else
      Delay(30)
    End If
  Wend
End Function
